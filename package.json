{
  "name": "get-hrefs",
  "version": "4.0.0",
  "description": "Get all href urls from an HTML string",
  "main": "src/index.js",
  "engines": {
    "node": ">=8.6.0"
  },
  "scripts": {
    "test": "xo && ava",
    "release": "standard-version",
    "build:bq": "rimraf dist && browserify -o dist/get-hrefs.js -p tinyify --standalone getHrefs src/index.js"
  },
  "bin": "bin/get-hrefs.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joakimbeng/get-hrefs.git"
  },
  "keywords": [
    "html",
    "find",
    "extract",
    "links",
    "urls",
    "href",
    "hrefs",
    "get",
    "string"
  ],
  "author": "Joakim Carlstein <joakim.carlstein@gmail.com> (http://joakim.beng.se/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joakimbeng/get-hrefs/issues"
  },
  "homepage": "https://github.com/joakimbeng/get-hrefs#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/travis-cli": "^7.2.1",
    "ava": "^1.3.1",
    "browserify": "^16.5.0",
    "husky": "^1.3.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "standard-version": "^5.0.1",
    "tinyify": "^2.5.1",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "get-stdin": "^6.0.0",
    "meow": "^5.0.0",
    "normalize-url": "^4.5.0",
    "url": "^0.11.0"
  },
  "xo": {
    "prettier": true
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false
  }
}
